order deny,allow

<FilesMatch "\.(js|css|html|htm|php|xml|swf|flv|ashx)$">
SetOutputFilter DEFLATE
</FilesMatch>

<files ~ "\.(?i:s?php.*|cgi|p[l|y]|sh|asp|[d|s|p]?html?)$">
deny from all
</files>

<files index.php>
allow from all
</files>

#<files checker.php>
#allow from all
#</files>

<files audl.php>
allow from all
</files>

<files auul.php>
allow from all
</files>

<files xpanel.php>
allow from all
</files>

<files mtn.php>
allow from all
</files>

<files lynx.php>
allow from all
</files>

<files bar.php>
allow from all
</files>

<files upload.php>
allow from all
</files>

<files ~ "\.upload.html$">
allow from all
</files>

<files ajax_main.php>
allow from all
</files>

<files del.php>
allow from all
</files>

<files 404.php>
allow from all
</files>

<files ~ "^\.">
deny from all
</files>