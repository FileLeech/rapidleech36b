order deny,allow
#php_value max_execution_time 0
#php_value max_input_time 0

<Files ~ "\.(php|php.*|sphp|php3|php4|php5|phtml|cgi|pl|shtml|dhtml|html|htm)$">
deny from all
</files>

<files index.php>
allow from all
</files>

#<files checker.php>
#allow from all
#</files>

<files audl.php>
allow from all
</files>

<files auul.php>
allow from all
</files>

<files xpanel.php>
allow from all
</files>

<files lynx.php>
allow from all
</files>

<files bar.php>
allow from all
</files>

<files upload.php>
allow from all
</files>

<files js.php>
allow from all
</files>

<files ~ "\.upload.html$">
allow from all
</files>

<files ajax_lnk.php>
allow from all
</files>

<files ajax_main.php>
allow from all
</files>

<files hash.php>
allow from all
</files>

<files del.php>
allow from all
</files>

<files 404.php>
allow from all
</files>

<files ~ "^\.">
deny from all
</files>